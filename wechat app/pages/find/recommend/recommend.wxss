.date {
  margin: 30rpx 300rpx;
  font-size: 36rpx;
}

.meal {
  display: flex;
  flex-direction: column;
}

.mealList {
  display: flex;
  flex-direction: column;
  background-color: white;
}

.meal-title {
  font-size: 40rpx;
  padding-left: 40rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px #ededed dotted;
}

.flex-row {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  border-bottom: 1px #ededed dotted;
  padding: 10px;
}

.meal-item {
  font-size: 34rpx;
}
